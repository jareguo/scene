{
  "name": "scene",
  "version": "0.0.1",
  "description": "A scene editor",
  "author": "Firebox Technology",
  "hosts": {
    "editor-framework": "^0.3.5"
  },
  "main": "main.js",
  "menus": {
    "Panel/Scene": {
      "icon": "panel/icon.png",
      "message": "scene:open"
    }
  },
  "panels": {
    "panel": {
      "frame": "panel/scene.html",
      "type": "dockable",
      "title": "Scene",
      "icon": "panel/icon.png",
      "width": "auto",
      "height": "auto",
      "messages": [
        "scene:play",
        "scene:stop",
        "editor:dragstart",
        "editor:dragend",
        "editor:state-changed"
      ],
      "profiles": {
        "local": {
          "collapses-status": []
        }
      }
    }
  },
  "tests": [
    "test/basic.html"
  ]
}
